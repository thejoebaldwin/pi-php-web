echo "11" > /sys/class/gpio/export
echo "out" > /sys/class/gpio/gpio11/direction
echo "1" > /sys/class/gpio/gpio11/value


for VARIABLE in 2 3 4 17 27 22 10 9 11 14 15 18 23 24 25 8 7
do
	
echo "$VARIABLE" > /sys/class/gpio/export
echo "out" > /sys/class/gpio/gpio$VARIABLE/direction
echo "1" > /sys/class/gpio/gpio$VARIABLE/value


done